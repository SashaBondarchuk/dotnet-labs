<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="Chauffeurs">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Chauffeur" maxOccurs="unbounded" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element type="xs:int" name="Id"/>
							<xs:element type="xs:string" name="Name"/>
							<xs:element type="xs:string" name="Surname"/>
							<xs:element type="xs:string" name="MiddleName"/>
							<xs:element type="xs:string" name="Adress"/>
							<xs:element type="xs:dateTime" name="BirthDate"/>
							<xs:element type="xs:string" name="DriverLicense"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>